<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dev334.litelo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SET_ALARM" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_green"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.dev334.litelo.userFeedback"
            android:screenOrientation="portrait"></activity>
        <activity android:name="com.dev334.litelo.subjectResources"
            android:screenOrientation="portrait"/>
        <activity android:name="com.dev334.litelo.AttendanceHistory"
            android:screenOrientation="portrait"/>
        <activity android:name="com.dev334.litelo.ClubClasses"
            android:screenOrientation="portrait"/>
        <activity android:name="com.dev334.litelo.PDFViewPage"
            android:screenOrientation="portrait"/>
        <activity android:name="com.dev334.litelo.Settings"
            android:screenOrientation="portrait"/>
        <activity android:name="com.dev334.litelo.createPDF"
            android:screenOrientation="portrait"/>
        <activity android:name="com.dev334.litelo.SubjectAttendance"
            android:screenOrientation="portrait" />
        <activity android:name="com.dev334.litelo.MainActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.dev334.litelo.UserInfo"
            android:screenOrientation="portrait" />
        <activity android:name="com.dev334.litelo.phone_login"
            android:screenOrientation="portrait"/>
        <activity android:name="com.dev334.litelo.profileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.dev334.litelo.HomeActivity"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.dev334.litelo.email_Login"
            android:screenOrientation="portrait"
            />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name="com.dev334.litelo.splashScreen"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.dev334.litelo.services.MyFirebaseInstanceService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"
                    />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-9915472110094523~2090670666" />

        <receiver android:name="com.dev334.litelo.Notification_receiver" />
    </application>

</manifest>